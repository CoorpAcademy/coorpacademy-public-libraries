{
  "name": "@coorpacademy/io-ts",
  "version": "1.0.0",
  "description": "Coorpacademy io-ts library",
  "main": "src/index.js",
  "types": "./src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepack": "npm run build",
    "test": "npm run lint && npm run typecheck && nyc npm run test:unit",
    "test:unit": "NODE_ENV=test mocha -r ts-node/register --exit test/*.unit.spec.ts",
    "lint": "eslint . --ext .js,.json,.ts",
    "typecheck": "tsc --noEmit",
    "publish-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "coorpacademy",
    "io-ts",
    "decoders",
    "codecs"
  ],
  "files": [
    "src",
    "LICENSE.txt",
    "README.md",
    "package.json"
  ],
  "author": "Coorpacademy Team",
  "license": "MIT",
  "homepage": "https://github.com/CoorpAcademy/coorpacademy-public-libraries/tree/master/packages/io-ts#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Coorpacademy/coorpacademy-public-libraries.git",
    "directory": "packages/acl"
  },
  "bugs": {
    "url": "https://github.com/Coorpacademy/coorpacademy-public-libraries/issues/new"
  },
  "dependencies": {
    "@types/validator": "^13.1.0",
    "fp-ts": "^2.9.0",
    "io-ts": "^2.2.13",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@coorpacademy/eslint-plugin-coorpacademy": "^10.2.0",
    "@types/chai": "^4.2.12",
    "@types/lodash": "^4.14.161",
    "@types/mocha": "^8.0.4",
    "@types/node": "^10.17.35",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "chai": "^4.2.0",
    "codecov": "^3.7.2",
    "eslint": "^7.9.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
